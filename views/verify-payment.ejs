          <!DOCTYPE html>
          <html>
          <head>
            <title>Verify Payment - RetailExper Services</title>
            <link rel="stylesheet" href="/style.css">
          </head>
          <body>
            <header>
              <h1>RetailExpert Services</h1>
              <nav>
                <ul>
                  <li><a href="/">Home</a></li>
                  <li><a href="/verify-payment">Verify Payment</a></li>
                </ul>
              </nav>
            </header>
            <main>
              <h1>Verify Payment</h1>
              <table>
                <thead>
                  <tr>
                    <th>Transaction ID</th>
                    <th>Customer</th>
                    <th>Status</th>
                    <th>Amount</th>
                    <th>Paid Date</th>
                  </tr>
                </thead>
                <tbody>
                  <% transactions.forEach(transaction => { %>
                    <tr>
                      <td><%= transaction.id %></td>
                      <td><%= transaction.customer %></td>
                      <td><%= transaction.status %></td>
                      <td>$<%= transaction.amount.toFixed(2) %></td>
                      <td><%= transaction.paidDate %></td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </main>
            <footer>
              <p>&copy; 2024 Tarek Retail Services. All rights reserved.</p>
            </footer>
          </body>
          </html>
